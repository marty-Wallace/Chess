package board;
import piece.Piece;

public class Square {
	
	private boolean empty;
	private Piece piece;
	private Location loc;
	
	public Square(Location loc){
		this.loc = loc;
		this.empty = true;
		this.piece = null;
	}
	
	public boolean isEmpty(){
		return this.empty;
	}
	
	public Location getLocation(){
		return this.loc;
	}
	public void setFilled(){
		this.empty = false;
	}
	public void setEmpty() {
		this.empty = true;
	}

}
